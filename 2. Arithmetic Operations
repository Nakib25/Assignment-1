# Arithmetic Operations
age = 26
number = 100

print("Age =", age)
print("Number =", number)

# Multiplication
print("Multiplication (age * number):", age * number)

# Addition
print("Addition (age + number):", age + number)

# Subtraction
print("Subtraction (number - age):", number - age)

# Division
print("Division (number / age):", number / age)
